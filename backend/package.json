  "main": "server.js",
  "scripts": {
    "build": "tsc --sourceMap false && cp -r src/config dist/ && cp -r src/models dist/",
    "devbuild": "tsc --sourceMap true",
    "watch": "tsc -w",
    "start": "node dist/server.js",
    "dev": "nodemon dist/server.js",
    "dev:server": "ts-node-dev --inspect --respawn --transpile-only --ignore node_modules src/server.ts",
    "db:migrate": "npx sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "pretest": "NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:all",
    "test": "NODE_ENV=test jest",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all",
    "lint": "eslint src/**/*.ts",
    "mark-seeds": "node dist/database/markSeedsAsExecuted.js"
  },
